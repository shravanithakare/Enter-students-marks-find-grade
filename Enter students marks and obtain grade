#include<stdio.h>
main()
{
	printf("Enter the marks of student:->");
	int marks=0;
	scanf("%d" ,&marks);
	if(marks<=100 && marks>85)
	{
		printf("Grade A");
	}
	else if(marks<=74&& marks>55)
	{
		printf("Grade B");
	}
	else if(marks<=55 && marks>40)
	{
		printf("Grade C");
	}
	else if(marks<=40 && marks>20)
	{
		printf("Grade D");
	}
	else	{
		printf("Grade F");
	}
}
